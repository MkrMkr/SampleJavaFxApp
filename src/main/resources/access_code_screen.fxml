<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<AnchorPane id="anchor" fx:id="anchorPane"
            style="-fx-background-image: url(splashscreen_bg.jpg); -fx-background-size: 100% 100%;"
            xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="AccessCodeController">
    <children>
        <Button fx:id="enterCheersButton" id="red" layoutX="68.0" layoutY="605.0" mnemonicParsing="false"
                onAction="#enterCheers"
                prefHeight="25.0" prefWidth="265.0" stylesheets="@red_button_style.css"
                text="Enter Cheers"
                AnchorPane.bottomAnchor="50" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"/>
        <TextField layoutX="67.0" layoutY="558.0" prefHeight="25.0" prefWidth="266.0" promptText="Access code"
                   AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"/>
        <ImageView fx:id="logoImageView" fitHeight="150.0" fitWidth="200.0" layoutY="5.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@cheers_logo.png"/>
            </image>
        </ImageView>
    </children>
</AnchorPane>
